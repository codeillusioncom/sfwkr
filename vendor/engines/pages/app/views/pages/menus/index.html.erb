<h1>Menus</h1>

<table class="table table-striped table-responsive table-hover table-bordered">
  <thead>
  <tr>
    <th>Type</th>
  </tr>
  </thead>
  <tbody>
  <% @menus.each do |menu| %>
    <tr>
      <td>
        <%= link_to menu.menu_type, pages.menu_menu_items_path(menu) %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>